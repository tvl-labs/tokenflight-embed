{
  "name": "tokenflight-embed",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "oxlint .",
    "check-types": "turbo run check-types",
    "test": "turbo run test",
    "docs:llms": "pnpm --filter @tokenflight/docs run generate:llms",
    "e2e": "playwright test",
    "e2e:update": "playwright test --update-snapshots"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "oxlint": "^0.16.6",
    "turbo": "^2.8.9",
    "typescript": "5.9.2"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=18"
  }
}
