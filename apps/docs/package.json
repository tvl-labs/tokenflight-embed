{
  "name": "@tokenflight/docs",
  "private": true,
  "type": "module",
  "scripts": {
    "generate:llms": "node ./scripts/generate-llms.mjs",
    "generate:search-index": "astro build && rm -rf ./public/pagefind && cp -R ./dist/pagefind ./public/pagefind",
    "predev": "pnpm run generate:llms && pnpm run generate:search-index",
    "dev": "astro dev --force",
    "prebuild": "pnpm run generate:llms",
    "build": "astro build",
    "preview": "astro preview"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.12",
    "@astrojs/starlight": "^0.34",
    "@fontsource/dm-sans": "^5.2.8",
    "@fontsource/jetbrains-mono": "^5.2.8",
    "@reown/appkit": "^1.8",
    "@reown/appkit-adapter-solana": "^1.8",
    "@reown/appkit-adapter-wagmi": "^1.8",
    "@tokenflight/adapter-appkit": "workspace:*",
    "@tokenflight/swap": "workspace:*",
    "astro": "^5.7",
    "prismjs": "^1.30.0",
    "viem": "^2",
    "wagmi": "^3"
  },
  "devDependencies": {
    "@types/prismjs": "^1.26.6"
  }
}
